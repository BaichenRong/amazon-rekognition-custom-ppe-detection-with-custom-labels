{
  "extends": ["airbnb-base"],
  "rules": {
    "strict": "error",
    "func-names": ["error", "never"],
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "always-multiline",
      "exports": "always-multiline",
      "functions": "never"
    }],
    "no-use-before-define": ["error", {
      "functions": false,
      "classes": true,
      "variables": true
    }],
    "prefer-arrow-callback": 0,
    "prefer-object-spread": 0,
    "arrow-parens": 0,
    "max-classes-per-file": 0,
    "implicit-arrow-linebreak": 0,
    "operator-linebreak": 0,
    "no-unused-vars": 0,
    "class-methods-use-this": 0,
    "no-plusplus": 0,
    "no-continue": 0,
    "no-alert": 0,
    "no-console": 0,
    "max-len": 0,
    "no-nested-ternary": 0,
    "no-await-in-loop": 0,
    "no-loop-func": 0,
    "no-restricted-globals": 0,
    "prefer-destructuring": 0,
    "no-restricted-properties": 0,
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "import/no-absolute-path": 0
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jquery": true
  },
  "globals": {
    "AWS": true,
    "AWSCognito": true,
    "AWSIoTData": true,
    "CryptoJS": true,
    "location": true,
    "AWSomeNamespace": true,
    "CardCollection": true,
    "VideoCard": true,
    "SystemMessageArea": true,
    "SettingsTabPanel": true,
    "MyCognito": true,
    "SignInModal": true,
    "FileUpload": true,
    "FileUploader": true,
    "CustomVocabulary": true,
    "ConfigurationError": true,
    "SearchBox": true,
    "BasePreview": true,
    "VideoPreview": true,
    "ImagePreview": true,
    "Cropper": true,
    "SparkMD5": true,
    "AmazonCognitoIdentity": true,
    "ResetPasswordModal": true,
    "ImageEditor": true,
    "WorkteamWizard": true,
    "CardFactory": true,
    "BaseCard": true,
    "ImageCard": true,
    "GoogleMap": true,
    "google": true,
    "JsonParser": true,
    "DefaultJsonParser": true,
    "CustomJsonParser": true,
    "BaseWizard": true,
    "MediaUploadWizard": true,
    "JsonUploadWizard": true,
    "videojs": true,
    "LabelEditor": true,
    "AudioCard": true,
    "AudioPreview": true,
    "mxBaseProvider": true,
    "NotImplError": true,
    "DocumentCard": true,
    "DocumentPreview": true,
    "ModalLayout": true
  }
}
